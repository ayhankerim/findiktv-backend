      "type": "enumeration",
      "enum": [
        "stockmarket",
        "openmarket",
        "tmo",
        "all"
      ],
      "default": "stockmarket",
      "required": true
    },
    "approvalStatus": {
      "type": "enumeration",
      "enum": [
        "editorOnly",
        "userOnly",
        "approved",
        "all"
      ],
      "default": "editorOnly",
      "required": true
    }
